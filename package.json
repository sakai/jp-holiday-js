{
  "name": "jp-holiday",
  "version": "1.0.0",
  "description": "Japanese national holidays library for JavaScript/TypeScript",
  "author": "Kiyoshi Sakai <sakai@d4k.net>",
  "license": "MIT",
  "keywords": ["japanese", "holiday", "japan", "calendar", "typescript"],
  "repository": {
    "type": "git",
    "url": "https://github.com/sakai/jp-holiday-js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
